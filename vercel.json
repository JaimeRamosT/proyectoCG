{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd image_inpainting_web && npm install && npm run build",
  "devCommand": "cd image_inpainting_web && npm run dev",
  "installCommand": "cd image_inpainting_web && npm install",
  "outputDirectory": "image_inpainting_web/dist",
  "framework": null,
  "functions": {
    "api/index.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api",
      "dest": "/api/index.py"
    },
    {
      "src": "/api/upload",
      "dest": "/api/index.py"
    }
  ]
}
