{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd image_inpainting_web && npm install && npm run build",
  "outputDirectory": "image_inpainting_web/dist",
  "functions": {
    "api/index.py": {
      "memory": 3008,
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.py"
    }
  ]
}
